{
    "extends": "standard",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "graphql"
    ],
    "rules": {
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "graphql/template-strings": "off"
    },
    "env": {
        "browser": true
    }
}
